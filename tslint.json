{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "semicolon": [
      true,
      "always"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "max-line-length": [
      true,
      200
    ],
    "no-console": false,
    "no-shadowed-variable": false,
    "no-trailing-whitespace": false,
    "quotemark": false
  },
  "exclude": [
    "node_modules/**"
  ]
}



